
#include <R.h>
#include "myheader.h"


void cum2(double *etain, int *typein, double *cumout)
{
    double eta = etain[0];
    int *type = typein[0];

    switch (type) {
    case 1:
	if(eta>0)
		{
		cumout[0]= eta+log(exp(-eta)+1);
		break;
		}
	else
		{
		cumout[0]= log(1+exp(eta));
		break;
		}
    case 2:
        cumout[0]= exp(eta);
	break;
    default:
        error("unrecognized type");
	break;
    }
}

