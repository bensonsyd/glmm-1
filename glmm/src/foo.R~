
dyn.load("foo.so") #loads dynamically loadable library called foo.so



#call C function named foo
.C("foo", eta = as.double(0.5), type = "bernoulli", cppout = double(1), cumout=double(1))

.C("foo", eta = as.double(-0.5), type = "bernoulli", cppout = double(1), cumout=double(1))

.C("foo", eta = as.double(4.5), type = "poisson", cppout = double(1),cumout=double(1))

try(.C("foo", eta = as.double(4.5), type = "fish", cppout = double(1), cumout=double(1)))

