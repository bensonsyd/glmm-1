
#include <string.h>
#include <R.h>
#include "myheader.h"

void cfamscpp(double *etain, char **typein,  double *cppout)
{
    double eta = etain[0];
    char *type = typein[0];
    if (strcmp(type, "bernoulli") == 0)
	{
	cppout[0]=cpp(eta,BERNOULLI);
	}
    else if (strcmp(type, "poisson") == 0)
	{
	cppout[0]=cpp(eta,POISSON);
	}
    else
        error("unrecognized type");
}

