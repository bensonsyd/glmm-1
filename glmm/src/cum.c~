
#include <R.h>
#include "myheader.h"


double cum(double eta, int type)
{
    switch (type) {
    case BERNOULLI:
	double out;
	if(eta>0)
		{
		out=eta+log(exp(-eta)+1);
		}
	else
		{
		out=log(1+exp(eta));
		}
	return out;
    case POISSON:
        return exp(eta);
    default:
        error("unrecognized type");
    }
}

