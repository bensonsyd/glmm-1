
#include <string.h>
#include <R.h>
#include "myheader.h"

void cfamscp(double *etain, char **typein,  double *cpout)
{
    double eta = etain[0];
    char *type = typein[0];
    if (strcmp(type, "bernoulli") == 0)
	{
	cpout[0]=cp(eta,BERNOULLI);
	}
    else if (strcmp(type, "poisson") == 0)
	{
	cpout[0]=cp(eta,POISSON);
	}
    else
        error("unrecognized type");
}

