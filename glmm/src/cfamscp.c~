
#include <string.h>
#include <R.h>
#include "myheader.h"

void cfamscum(double *etain, char **typein,  double *cumout)
{
    double eta = etain[0];
    char *type = typein[0];
    if (strcmp(type, "bernoulli") == 0)
	{
	cumout[0]=cum(eta,BERNOULLI);
	}
    else if (strcmp(type, "poisson") == 0)
	{
	cumout[0]=cum(eta,POISSON);
	}
    else
        error("unrecognized type");
}

